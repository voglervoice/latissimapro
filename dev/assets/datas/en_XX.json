      "range__capsules":{
        "section":"range__capsules",
        "blockText":{
          "capsules_category_3_text": {
             "id": "7_13",
            "label":"capsules_category_3_text",
            "texte":"3 Grands Crus offering a wide range of taste profiles and intensities, specially designed for enjoyment in large cups (3.75 oz/110 ml). "
          },
          "capsules_category_4": {
             "id": "7_12",
            "label":"capsules_category_4",
            "texte":"Decaffeinato"
          },
          "capsules_category_2": {
             "id": "7_11",
            "label":"capsules_category_2",
            "texte":"Espresso"
          },
          "capsules_category_4_text": {
             "id": "7_10",
            "label":"capsules_category_4_text",
            "texte":"2 Espressos and 1 Grand Cru Lungo. Each of these authentic Nespresso coffees has its own character and rich aromas which have been preserved during the decaffeination process."
          },
          "capsules_category_3": {
             "id": "7_9",
            "label":"capsules_category_3",
            "texte":"Lungo"
          },
          "capsules_category_5": {
             "id": "7_8",
            "label":"capsules_category_5",
            "texte":"Pure origin"
          },
          "capsules_category_1": {
             "id": "7_5",
            "label":"capsules_category_1",
            "texte":"Intenso"
          },
          "capsules_category_6_text": {
             "id": "7_6",
            "label":"capsules_category_6_text",
            "texte":"3 gourmet variations on the Livanto Grand Cru, flavoured with natural aromas of exceptional finesse. Best enjoyed in a short cup (1.35 oz/40ml)."
          },
          "capsules_category_1_text": {
             "id": "7_7",
            "label":"capsules_category_1_text",
            "texte":"5 blends of various origins, offer a wide range of intense characters.  Ideally tasted in a very small cup (0.85 oz/25ml) to benefit from all the rich aromas or in a small cup (1.35 oz/40ml)."
          },
          "capsules_category_5_text": {
             "id": "7_3",
            "label":"capsules_category_5_text",
            "texte":"4 Espressos with unique and precious personalities. Each comes from a single origin in exceptional terroirs, specially selected by our Coffee Experts. To be served in a small cup (1.35 oz/40 ml)."
          },
          "capsules_category_6": {
             "id": "7_4",
            "label":"capsules_category_6",
            "texte":"Variations"
          },
          "capsules_category_2_text": {
             "id": "7_14",
            "label":"capsules_category_2_text",
            "texte":"4 creations with very different personalities. Each blend comes from several origins and is carefully selected by our Coffee Experts. To be served in a small cup (1.35 oz /40 ml)."
          }
        }
